@model AssetTransfer
@{
    ViewData["Title"] = "CreateAssetTransfer";
}

<h5 class="mt-3 text-center">Asset Transfer</h5>

<section class="bg-light mt-3 px-5 py-3">

    <div class="row">
        <div class="col-6">
            <div class="form-group">
                <label class="control-label">Transfer ID</label>
                <input class="form-control" />
            </div>
            <div class="form-group">
                <label class="control-label d-flex">SubSection ID</label>
                <select class="form-select" aria-label="Default select example">
                    <option selected>001</option>
                    <option value="1">002</option>
                    <option value="2">003</option>
                </select>
            </div>
            <div class="form-group">
                <label class="control-label">Purchase ID</label>
                <input class="form-control" />
            </div>
            <div class="form-group">
                <label class="control-label">From_Branch</label>
                <input class="form-control" />
            </div>
            <div class="form-group">
                <label class="control-label">To_Branch</label>
                <input class="form-control" />
            </div>
        </div>
    </div>
    
    <div class="my-3">
        <div class="d-inline-flex justify-content-center">
            <a asp-action="" class="btn btn-primary">Save</a>
            <a asp-controller="Dashboard" asp-action="Dashboard" class="btn btn-primary mx-3">Cancel</a>
        </div>
    </div>
</section>